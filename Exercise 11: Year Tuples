year = (2017,2003,2011,2005,1987,2009,2020,2018,2009)

startvalue = year [-3]
print("Value at index -3:", startvalue)

def Reverse(tuples):
    new_tup = tuples[::-1]
    return new_tup

print("Original Tuple:", year)
print("Reversed Tuple:",Reverse(year))

x = year.count(2009)
print("Count of 2009:", x)

x = year.index(2018)
print("Index of 2018:", x)

x = len(year)
print("Length of Tuple:", x)
